\section{SURVEY}
\subsection{DATA STRUCTURE}
\begin{frame}
\begin{itemize}
  \item survey was conducted using an internal system known as REDCap.
  \item sample size = 60
  \item the distribution of the respondents is as shown below  
\end{itemize}
\begin{figure}
<<echo=FALSE,fig.height=3.2,fig.width=7.2>>=
ggplot(survey[!is.na(survey$role),], aes(x = factor(role))) + geom_bar(stat = "bin",fill="darkblue")+ scale_x_discrete(labels=c("INT","RA/AS","R.PHD","PD.SC","SEN.INV","STATS","OTHER"))

@

\end{figure}
\end{frame}

\subsection{PROFICIENCY LEVELS}
\begin{frame}[fragile]{\textbf{GROUP1}}
\begin{figure}
<<echo=FALSE,fig.height=4.2,fig.width=7.2>>=
survey$group[survey$role==1] <- "interns" 
survey$group[survey$role!=1] <- "noninterns"
tb1.1<-table(survey$proflvl,survey$group)
p1.1<-ggplot(survey[!is.na(survey$group),],aes(proflvl, fill=group)) + geom_bar(position="dodge", binwidth=0.5)
p1.1+ scale_x_discrete(breaks=1:4,labels=c("veryproficient","proficient","moderate","novice" )) + ggtitle("DISTRIBUTION OF  LEVELS OF STATISTICAL ANALYSIS") + xlab("Proficiency levels")

@

\end{figure}

\end{frame}

\begin{frame}[fragile]{\textbf{GROUP 2}}
\begin{figure}
<<echo=FALSE,fig.height=4.2,fig.width=7.2>>=
survey$edu[survey$edulvl==1]<-"bachelors"
survey$edu[survey$edulvl==2]<-"masters"
survey$edu[survey$edulvl==3]<-"phd"
survey$edu[survey$edulvl==4]<-"bachelors"

p1.2<-ggplot(survey[!is.na(survey$edu),],aes(proflvl, fill=edu)) + geom_bar(position="dodge", binwidth=0.5)
p1.2+ scale_x_discrete(breaks=1:4,labels=c("veryproficient","proficient","moderate","novice" ))+ggtitle("DISTRIBUTION OF LEVELS OF STATISTICAL ANALYSIS") + xlab("Proficiency levels")
@

\end{figure}
\end{frame}

\subsection{PROJECT RATING}
